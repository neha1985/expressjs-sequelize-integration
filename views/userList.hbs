<h2>{{Title}}</h2>
<a class="btn btn-success" href="/users/create">Create</a>
<hr>
<table class="table table-hover">
    <thead>
        <tr>
            <th>User Id</th>
            <th>Name</th>
            <th>Address</th>
            <th>Email</th>
            <th>Phone</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        {{#each data}}
        <tr>
            <td>{{this.id}}</td>
            <td>{{this.name}}</td>
            <td>{{this.address}}</td>
            <td>{{this.email}}</td>
            <td>{{this.phone}}</td>
             <td>
                <a href="/user/edit/{{this.userId}}" class="btn btn-success">Edit</a>    
                <a href="/user/delete/{{this.userId}}" class="btn btn-warning">Delete</a>    
             </td>
        </tr>
        {{/each}}
    </tbody>
</table>
